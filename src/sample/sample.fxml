<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="552.0" prefWidth="671.0">
         <children>
            <TableView fx:id="tableShip" layoutX="14.0" layoutY="86.0" prefHeight="373.0" prefWidth="644.0">
              <columns>
                  <TableColumn fx:id="ID" prefWidth="42.0" text="ID" />
                <TableColumn fx:id="Berth_No" prefWidth="98.0" text="Berth_No" />
                <TableColumn fx:id="Bollard_No" prefWidth="71.0" text="Bollard_No" />
                  <TableColumn fx:id="Name" prefWidth="89.0" text="Name" />
                  <TableColumn fx:id="ETA" prefWidth="106.0" text="ETA" />
                  <TableColumn fx:id="ETD" prefWidth="93.0" text="ETD" />
                  <TableColumn fx:id="Last_Port" prefWidth="74.0" text="Last_Port" />
                  <TableColumn fx:id="Next_Port" prefWidth="70.0" text="Next_Port" />
              </columns>
            </TableView>
            <Button fx:id="btnDisplay" layoutX="155.0" layoutY="470.0" mnemonicParsing="false" onAction="#loadDataFromDatabase" text="Display" />
         </children></Pane>
   </children>
</GridPane>
